<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activities.PcelinjakSingleActivity">

<com.tbuonomo.viewpagerdotsindicator.WormDotsIndicator
    android:id="@+id/dots_indicatorSinglePagePcelinjak"
    android:layout_width="346dp"
    android:layout_height="23dp"
    android:layout_marginStart="80dp"
    android:layout_marginTop="9dp"
    app:dotsColor="#DFAF36"
    app:dotsCornerRadius="20dp"
    app:dotsSize="20dp"
    app:dotsSpacing="20dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:progressMode="true" />

<com.google.android.material.floatingactionbutton.FloatingActionButton
    android:id="@+id/fab_backSinglePagePcelinjak"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="30dp"
    android:layout_marginEnd="5dp"
    android:elevation="8dp"
    app:fabSize="mini"
    app:layout_constraintEnd_toStartOf="@+id/dots_indicatorSinglePagePcelinjak"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

<androidx.viewpager2.widget.ViewPager2
    android:id="@+id/viewPagerSinglePagePcelinjak"
    android:layout_width="424dp"
    android:layout_height="557dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/fab_backSinglePagePcelinjak" />

</androidx.constraintlayout.widget.ConstraintLayout>